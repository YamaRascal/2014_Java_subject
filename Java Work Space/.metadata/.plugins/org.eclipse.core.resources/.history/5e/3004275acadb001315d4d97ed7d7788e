public class Ex025 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int x,y;
		x =Integer.MIN_VALUE ;
		y =Integer.MAX_VALUE ;
		
		
		System.out.println(("int型の最小値 -1 = ") + (x )) ;
		System.out.println(("int型の最大値 +1 = ") + (y )) ;

		
	}

}
