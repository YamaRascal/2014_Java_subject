import java.util.Scanner;
public class Factorial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		
				
		int x =scan.nextInt();
		
		long fac=fac(x);
		System.out.println(x+"の階乗は＞"+fac);
		
	}
	static long fac(int x){
		long max=1;
		for(int i=0;i<x;i++){
			max *=x-i;
		}
		return max;
		
	}
	
	
}
