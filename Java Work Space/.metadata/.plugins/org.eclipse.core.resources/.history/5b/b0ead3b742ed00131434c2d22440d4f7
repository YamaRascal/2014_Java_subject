
public class Ex064_teacher01 {
		public static void main(String[]augs){
			int escape;
			int count =0;
			int [] num = new int[52];
			int len = num.length;
			
			for(int i=0;i<len;i++){
				num[i]=i+1;
			}
			
			for(int i = 0;i<5;i++){
				int n = (int)(Math.random()*len);
				System.out.println(num[n]);
				escape =num[n];
				num[n]=num[len];
				num[len]=escape;
				len--;
			}
			
		}
}
